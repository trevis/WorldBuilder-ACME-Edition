<UserControl xmlns="https://github.com/avaloniaui"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
             xmlns:vm="using:WorldBuilder.Editors.Landscape.ViewModels"
             xmlns:v="using:WorldBuilder.Editors.Landscape.Views"
             mc:Ignorable="d" d:DesignWidth="300" d:DesignHeight="400"
             x:Class="WorldBuilder.Editors.Landscape.Views.PasteSubToolView"
             x:DataType="vm:PasteSubToolViewModel">
    <UserControl.KeyBindings>
        <KeyBinding Gesture="OemOpenBrackets" Command="{Binding RotateCounterClockwiseCommand}" />
        <KeyBinding Gesture="OemCloseBrackets" Command="{Binding RotateClockwiseCommand}" />
        <KeyBinding Gesture="R" Command="{Binding RotateClockwiseCommand}" />
        <KeyBinding Gesture="Shift+R" Command="{Binding RotateCounterClockwiseCommand}" />
    </UserControl.KeyBindings>
    <v:StampLibraryPanel DataContext="{Binding}" />
</UserControl>
